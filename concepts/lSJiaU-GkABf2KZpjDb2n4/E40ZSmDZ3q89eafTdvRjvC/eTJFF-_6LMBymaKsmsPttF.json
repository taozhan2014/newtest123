  "vars" : [ {
    "name" : "htmlTemplate",
    "expr" : {
      "str" : "<html>\n  <head>\n    <title>ConceptJS Course {{{courseNumber}}}</title>\n    <link rel=\"stylesheet\" href=\"/resourcelocal://LCmYPBCjTiAj6aUOc9MJ99/\">\n    <script src=\"/resourcelocal://9_xE72qLWW9owaoIrWz3G5/\"></script>\n        \n    <meta name=\"cjsSaveMode\" content=\"script\">\n    <meta name=\"cjsHeaderActive\" content=\"yes\">\n    <script>\n        var anchorUri = \"{{{anchorUri}}}\";\n    </script>\n  </head>\n\n  <body class=\"container-fluid\">\n      <div id=\"fb-root\"></div>\n        <script>(function(d, s, id) {\n            var js, fjs = d.getElementsByTagName(s)[0];\n            if (d.getElementById(id)) return;\n            js = d.createElement(s); js.id = id;\n            js.src = \"//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.0\";\n            fjs.parentNode.insertBefore(js, fjs);\n            }(document, 'script', 'facebook-jssdk'));\n        </script>\n        \n        <script>\n            window.twttr = (function (d, s, id) {\n                var t, js, fjs = d.getElementsByTagName(s)[0];\n                if (d.getElementById(id)) return;\n                js = d.createElement(s); js.id = id;\n                js.src= \"https://platform.twitter.com/widgets.js\";\n                fjs.parentNode.insertBefore(js, fjs);\n                return window.twttr || (t = { _e: [], ready: function (f) { t._e.push(f) } });\n            }(document, \"script\", \"twitter-wjs\"));\n        </script>\n        \n    <div class=\"col-xs-1 col-sm-1 col-md-2\"></div>\n\n    <div class=\"col-xs-10 col-sm-10 col-md-8\">\n        <div class=\"row page-header\">\n            <div class=\"col-sm-10 col-lg-10 col-md-10\">\n                <h2><i class=\"fa fa-university\"></i> ConceptJS University</h2>\n            </div>\n            <div class=\"col-sm-2 col-lg-2 col-md-2 sociallinks\">\n                <ul>\n                    <li>\n                        <div class=\"fb-like\" \n                            data-href=\"https://www.facebook.com/conceptjs\" \n                            data-layout=\"button\" \n                            data-action=\"like\" \n                            data-show-faces=\"false\" \n                            data-share=\"false\">\n                        </div>\n                    </li>\n                    <li>\n                        <a class=\"twitter-share-button cjs-twitter-button\" href=\"https://twitter.com/lambdazen\"\n                            data-url=\"https://www.conceptjs.com\" \n                            data-text=\"Check out ConceptJS to use, build and share web applications directly through the browser\"\n                            data-hashtags=\"conceptjs\"\n                            data-count=\"none\"\n                            data-lang=\"en\">\n                            Tweet\n                        </a>\n                    </li>\n                </ul>\n            </div>\n        </div>\n\n      <div>\n        <h4 style=\"padding-top: 20px\">Course {{{courseNumber}}}: {{courseName}}</h4>\n        <div class=\"progress\" style=\"width: 100px; float: right;\">\n          <div class=\"progress-bar\" role=\"progressbar\" aria-valuenow=\"60\" aria-valuemin=\"0\" aria-valuemax=\"100\" data-bind=\"attr: {style: completeWidth}\">\n          </div>\n        </div>\n        <div style=\"float: right; padding: 2px 5px 2px 10px; font-size: small; vertical-align: middle;\"><span data-bind=\"text: completePercent\"></span>%</div>\n\n        <h4>Unit <span data-bind=\"text: unitNumber\"></span>: <span data-bind=\"text: unitName\"></span></h4>\n      </div>\n\n      <br>\n      <div class=\"row\">\n        <div data-bind=\"template: { name: 'prev-template' }\" style=\"float: left\"></div>\n\n        <button type=\"button\" class=\"btn btn-default btn-sm\" data-bind=\"click: nextClick, visible: nextEnabled\" style=\"float: right\"><i class=\"fa fa-arrow-right\"></i> &nbsp; Next Unit</button>\n        <button type=\"button\" class=\"btn btn-default btn-sm\" data-bind=\"click: doneClick, visible: doneEnabled\" style=\"float: right\"><i class=\"fa fa-thumbs-o-up\"></i> &nbsp; Done!</button>\n\n<!--        <button type=\"button\" class=\"btn btn-default btn-sm\" data-bind=\"click: prevClick, visible: prevEnabled\" style=\"float: left\"><i class=\"fa fa-arrow-left\"></i> &nbsp; Previous Unit</button> -->\n<!--        <button type=\"button\" class=\"btn btn-default btn-sm\" data-bind=\"click: goBackClick\" style=\"float: right\"><i class=\"fa fa-eject\"></i> &nbsp; Exit and continue later</button> -->\n      </div>\n\n      <br>\n\n      <div class=\"row\">\n        <div class=\"content\" data-bind=\"html: unitContent\"></div>\n      </div>\n\n      <br>\n      <div class=\"row\">\n        <div data-bind=\"template: { name: 'prev-template' }\" style=\"float: left\"></div>\n\n        <button type=\"button\" class=\"btn btn-default btn-sm btn-primary\" data-bind=\"click: nextClick, visible: nextEnabled\" style=\"float: right\"><i class=\"fa fa-arrow-right\"></i> &nbsp; Next Unit</button>\n        <button type=\"button\" class=\"btn btn-default btn-sm btn-primary\" data-bind=\"click: doneClick, visible: doneEnabled\" style=\"float: right\"><i class=\"fa fa-thumbs-o-up\"></i> &nbsp; Done!</button>\n      </div>\n      <br><br><br>\n    </div>\n\n    <div class=\"col-xs-1 col-sm-1 col-md-2\"></div>\n\n    <script type=\"text/html\" id=\"prev-template\">\n      <button type=\"button\" class=\"btn btn-default btn-sm\" data-bind=\"click: goBackClick, visible: ejectEnabled\"><i class=\"fa fa-eject\"></i> &nbsp; Exit and continue later</button>\n      <div class=\"btn-group\" data-bind=\"visible: prevEnabled\">\n        <button type=\"button\" class=\"btn btn-default btn-sm\" data-bind=\"click: prevClick\"><i class=\"fa fa-arrow-left\"></i> &nbsp; Previous Unit</button>\n        <button type=\"button\" class=\"btn btn-default btn-sm dropdown-toggle\" data-toggle=\"dropdown\">\n          &thinsp;<span class=\"caret\"></span>&thinsp;\n        </button>\n        <ul class=\"dropdown-menu\" role=\"menu\">\n          <li><a href=\"javascript: void(0)\" data-bind=\"click: startClick\"><i class=\"fa fa-backward\"></i> &nbsp; Start from beginning</a></li>\n          <li><a href=\"javascript: void(0)\" data-bind=\"click: goBackClick\"><i class=\"fa fa-eject\"></i> &nbsp; Exit</a></li>\n        </ul>\n      </div>\n    </script>\n    <script>\nvar cvm = $cjs.ko(\"{{{modelUri}}}\");\n\nvar courseModel = new CourseReaderModel(cvm, null);\nko.applyBindings(courseModel);\n\nvar readerType = cvm.readerType();\n//console.log(\"Reader type: \" + readerType);\n$('.progPanel,.dataPanel,.mathPanel,.nonePanel').collapse('hide').on('show.bs.collapse', function() {\n    $(this).parent().find('.panel-title > a > i').removeClass('fa-caret-right').addClass('fa-caret-down');\n}).on('hide.bs.collapse', function() {\n    $(this).parent().find('.panel-title > a > i').removeClass('fa-caret-down').addClass('fa-caret-right');\n});\n\n$(function() {\n    if (readerType == 'programmer') {\n        $('.progPanel').collapse('show');\n    } else if (readerType == 'dataScientist') {\n        $('.dataPanel').collapse('show');\n    } else if (readerType == 'mathematician') {\n        $('.mathPanel').collapse('show');\n    } else {\n        $('.nonePanel').collapse('show');\n    }\n});    </script>\n  </body>\n</html>",
      "strEditMode" : "html"
    }
  }, {
    "name" : "_page_propertyMap",
    "expr" : {
      "script" : "return {\"modelUri\": model.uri(),\n    \"anchorUri\": model.anchor().uri(),\n    \"courseNumber\": model.prop('course').prop('number').val(),\n    \"courseName\": model.prop('course').prop('name').val(),\n    \"curUnitNumber\": model.prop('curUnitNumber').val(),\n    \"readerType\": model.prop('readerType').val(),\n    \"returnUri\": model.prop('returnUri').val()};"
    }
  }, {
    "name" : "page",
    "expr" : {
      "concept" : "/common/core/Xfh2VQ4HSK9klqbqqR-eBB/",
      "bindings" : {
        "htmlTemplate" : "htmlTemplate",
        "propertyMap" : "_page_propertyMap"
      }
    }
  } ],
  "isas" : [ {
    "var" : "page"
  } ]
