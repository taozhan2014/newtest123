      "bindings" : {
        "htmlTemplate" : "htmlTemplate",
        "propertyMap" : "_page_propertyMap"
      }
    }
  } ],
  "isas" : [ {
    "var" : "page"
  } ],
  "isLambda" : true,
  "digest" : "pJaMUp1QpKg63je7Ol2e0A=="
